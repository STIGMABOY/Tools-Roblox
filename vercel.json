{
  "version": 2,
  "builds": [
    {
      "src": "api/check.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "api/admin.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/check",
      "dest": "/api/check.py",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/api/check/(.*)",
      "dest": "/api/check.py",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/api/admin",
      "dest": "/api/admin.py",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/api/admin/(.*)",
      "dest": "/api/admin.py",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization"
        }
      ]
    }
  ]
}
